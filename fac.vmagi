        addi #1, 15, #1  -- 0
        push #1          -- 1
        call @fac        -- 2
        pop #1           -- 3
        halt #1          -- 4

fac:    pop #1           -- 5
        jmpz #1, @base   -- 6
        subi #1, 1, #2   -- 7
        push #2          -- 8
        call @fac        -- 9
        pop #2           -- 10
        mul #1, #2, #1   -- 11
        push #1          -- 12
        ret

base:   pushi 1          -- 13
        ret              -- 14